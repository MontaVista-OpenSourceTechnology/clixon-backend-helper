<clixon-config xmlns="http://clicon.org/config">
  <CLICON_CONFIGFILE>@SYSCONFDIR@/clixon/@APPNAME@.xml</CLICON_CONFIGFILE>
  <CLICON_CONFIGDIR>@SYSCONFDIR@/clixon/@APPNAME@</CLICON_CONFIGDIR>
  <CLICON_FEATURE>ietf-netconf:startup</CLICON_FEATURE>
  <CLICON_FEATURE>clixon-restconf:allow-auth-none</CLICON_FEATURE>
  <CLICON_YANG_DIR>@DATADIR@/clixon</CLICON_YANG_DIR>
  <CLICON_YANG_MODULE_MAIN>@APPNAME@</CLICON_YANG_MODULE_MAIN>
  <CLICON_CLI_MODE>@APPNAME@</CLICON_CLI_MODE>
  <CLICON_CLISPEC_DIR>@LIBDIR@/@APPNAME@/clispec</CLICON_CLISPEC_DIR>
  <CLICON_BACKEND_DIR>@LIBEXECDIR@/clixon_beh</CLICON_BACKEND_DIR>
  <CLICON_SOCK>@LOCALSTATEDIR@/@APPNAME@/@APPNAME@.sock</CLICON_SOCK>
  <CLICON_BACKEND_PIDFILE>@LOCALSTATEDIR@/@APPNAME@/@APPNAME@.pidfile</CLICON_BACKEND_PIDFILE>
  <CLICON_PLUGIN_DLOPEN_GLOBAL>true</CLICON_PLUGIN_DLOPEN_GLOBAL>
  <!-- true here means restconf config is in datastore -->
  <CLICON_BACKEND_RESTCONF_PROCESS>true</CLICON_BACKEND_RESTCONF_PROCESS>
  <CLICON_XMLDB_DIR>@LOCALSTATEDIR@/@APPNAME@</CLICON_XMLDB_DIR>
  <CLIXON_TMPDB_VOLATILE>true</CLIXON_TMPDB_VOLATILE>
  <CLICON_STARTUP_MODE>init</CLICON_STARTUP_MODE>
  <CLICON_SOCK_GROUP>@CLICON_GROUP@</CLICON_SOCK_GROUP>
  <CLICON_NETCONF_HELLO_OPTIONAL>true</CLICON_NETCONF_HELLO_OPTIONAL>
  <CLICON_RESTCONF_USER>@CLICON_USER@</CLICON_RESTCONF_USER>
  <CLICON_RESTCONF_PRIVILEGES>drop_temp</CLICON_RESTCONF_PRIVILEGES>
  <CLICON_RESTCONF_INSTALLDIR>@SBINDIR@</CLICON_RESTCONF_INSTALLDIR>
</clixon-config>
